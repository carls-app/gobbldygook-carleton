{
  "name": "gobbldygook",
  "version": "3.0.0alpha8",
  "description": "A course scheduling app for St. Olaf College",
  "main": "dist/app.js",
  "browser": "dist/index.html",
  "private": true,
  "scripts": {
    "benchmark": "matcha",
    "build": "npm run clean && gulp build",
    "check": "npm outdated --depth 0",
    "clean": "rm -rf dist/ && rm -f node_modules/app && rm -f node_modules/sto-areas",
    "cloc": "cloc . --exclude-dir=data,node_modules,dist,test,playground,gulp --by-file-by-lang",
    "lint": "jscs ./app/**/*.js ./mockups/**/*.js ./gulp/**/*.js",
    "gulp": "gulp",
    "serve": "gulp watch",
    "stat": "npm run gulp lint && npm run benchmark && npm run test",
    "test": "mocha",
    "watch": "gulp watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hawkrives/gobbldygook.git"
  },
  "author": "Hawken MacKay Rives",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hawkrives/gobbldygook/issues"
  },
  "homepage": "https://hawkrives.github.io/gobbldygook",
  "dependencies": {
    "bluebird": "^2.5.3",
    "colors.css": "^2.0.4",
    "fetch": "https://github.com/github/fetch/archive/v0.3.2.tar.gz",
    "humanize-plus": "^1.5.0",
    "immutable": "^3.4.1",
    "lodash": "^2.4.1",
    "marked": "0.3.2",
    "node-uuid": "^1.4.2",
    "react": "^0.12.2",
    "react-dnd": "^0.6.2",
    "react-router": "^0.11.6",
    "reflux": "^0.2.1",
    "sto-courses": "^2.2.3",
    "treo": "^0.3.0"
  },
  "devDependencies": {
    "6to5": "^2.5.0",
    "6to5ify": "^3.1.0",
    "autoprefixer-core": "4.0.2",
    "browser-sync": "^1.8.3",
    "browserify": "^8.0.3",
    "csswring": "^2.0.0",
    "exorcist": "^0.1.6",
    "gulp": "^3.8.10",
    "gulp-changed": "^1.1.0",
    "gulp-if": "^1.2.5",
    "gulp-jscs": "^1.4.0",
    "gulp-notify": "^2.1.0",
    "gulp-postcss": "^3.0.0",
    "gulp-sass": "^1.2.4",
    "gulp-size": "^1.2.0",
    "gulp-sourcemaps": "^1.3.0",
    "gulp-symlink": "^2.1.0",
    "gulp-uglify": "^1.0.2",
    "gulp-util": "^3.0.1",
    "jscs": "^1.9.0",
    "jscs-jsdoc": "^0.4.0",
    "matcha": "^0.6.0",
    "mocha": "^2.1.0",
    "mold-source-map": "^0.3.0",
    "pretty-hrtime": "^1.0.0",
    "require-dir": "^0.1.0",
    "should": "^4.4.2",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^2.2.1"
  }
}
