{
  "name": "gobbldygook",
  "version": "3.0.0-alpha-12",
  "description": "A course scheduling app for St. Olaf College",
  "main": "app/index.js",
  "private": true,
  "repository": "hawkrives/gobbldygook",
  "author": "Hawken MacKay Rives",
  "license": "MIT",
  "config": {
    "browserify": "--debug -e app/index.js -t [ babelify --experimental ]",
    "uglify": "--source-map-root ./dist --in-source-map dist/app.js.map --source-map dist/app.min.js.map --compress --mangle --screw-ie8",
    "sass": "--stdout --source-map app.css.map --source-map-embed",
    "csswring": "--sourcemap"
  },
  "scripts": {
    "cloc": "cloc ./ --by-file-by-lang --exclude-dir=dist,node_modules,playground,test --force-lang=CSS,styl",
    "check": "npm outdated --depth 0",
    "clean": "rm -rf dist/",
    "copy-courses": "mkdir -p dist/data/courses/ && cp -f node_modules/sto-courses/info.json dist/data/courses && cp -f -R node_modules/sto-courses/terms dist/data/courses",
    "copy-html": "cp -f app/htdocs/index.html app/htdocs/.htaccess dist/",
    "copy-fonts": "cp -f -R app/fonts dist/",
    "copy-icons": "mkdir -p dist/fonts/ && cp -f app/icons/font/ionicons.woff dist/fonts/",
    "copy": "npm run copy-courses & npm run copy-html & npm run copy-fonts & npm run copy-icons; wait",
    "build-scripts": "browserify $npm_package_config_browserify | exorcist dist/app.js.map > dist/app.js",
    "build-styles": "node-sass app/styles/app.scss $npm_package_config_sass | autoprefixer > dist/app.css",
    "prebuild": "npm run lint",
    "build": "npm run copy & npm run build-scripts & npm run build-styles; wait",
    "watch-scripts": "watchify --verbose -o dist/app.js $npm_package_config_browserify",
    "watch-styles": "nodemon --watch app/styles --ext scss --exec npm run-script build-styles",
    "watch-lint": "nodemon --watch app --watch test --ext js --exec npm run lint",
    "prewatch": "npm run lint",
    "watch": "npm run copy & npm run watch-lint & npm run watch-scripts & npm run watch-styles; wait",
    "browser-sync": "browser-sync start --config ./bs-config.js",
    "csswring": "csswring $npm_package_config_csswring dist/app.css",
    "uglify": "uglifyjs dist/app.js --output dist/app.min.js $npm_package_config_uglify",
    "eslint": "eslint app/",
    "lint": "npm run eslint; wait",
    "test": "jest",
    "start": "npm run serve",
    "serve": "npm run watch & npm run browser-sync; wait",
    "travis": "npm run lint && npm run build && npm run test",
    "production": "npm run clean && npm run build && npm run uglify & npm run csswring; wait"
  },
  "dependencies": {
    "babel-core": "^4.7.16",
    "bluebird": "^2.9.15",
    "classnames": "^1.2.0",
    "combinations-generator": "^1.0.1",
    "humanize-plus": "^1.5.0",
    "idb-range": "^2.3.0",
    "immutable": "^3.6.4",
    "keymage": "^1.1.0",
    "lodash": "^3.6.0",
    "marked": "^0.3.2",
    "node-uuid": "^1.4.3",
    "present": "0.0.6",
    "ramda": "^0.13.0",
    "react": "^0.13.1",
    "react-dnd": "^0.9.8",
    "react-list-select": "^0.3.0",
    "react-router": "^0.13.2",
    "reflux": "^0.2.7",
    "stickyfill": "^1.1.1",
    "sto-areas": "^2.3.8",
    "sto-courses": "^3.1.0",
    "sto-helpers": "^4.2.7",
    "sto-sis-time-parser": "^2.0.0",
    "treo": "^0.4.3",
    "whatwg-fetch": "^0.7.0"
  },
  "devDependencies": {
    "autoprefixer": "^5.1.0",
    "babel-eslint": "^2.0.2",
    "babel-jest": "^4.0.0",
    "babelify": "^5.0.4",
    "browser-sync": "^2.4.0",
    "browserify": "^9.0.3",
    "csswring": "^3.0.2",
    "dom-storage": "^2.0.1",
    "eslint": "^0.17.1",
    "eslint-plugin-react": "^1.6.1",
    "exorcist": "^0.1.6",
    "jest-cli": "^0.4.0",
    "node-sass": "^2.0.1",
    "nodemon": "^1.3.7",
    "uglify-js": "^2.4.17",
    "watchify": "^2.6.2"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testDirectoryName": "test",
    "testPathDirs": [
      "test"
    ]
  }
}
