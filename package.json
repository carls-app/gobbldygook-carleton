{
  "name": "gobbldygook",
  "version": "3.0.0alpha6",
  "description": "A course scheduling app for St. Olaf College",
  "main": "build/app.js",
  "browser": "build/index.html",
  "private": true,
  "scripts": {
    "test": "mocha",
    "start": "make build"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "jshintConfig": {
    "asi": true,
    "browser": true,
    "curly": false,
    "devel": true,
    "expr": true,
    "eqeqeq": true,
    "esnext": true,
    "globalstrict": true,
    "indent": "\t",
    "loopfunc": true,
    "node": false,
    "trailing": true,
    "newcap": false,
    "quotmark": "single",
    "strict": true,
    "undef": true,
    "white": true,
    "predef": [
      "require",
      "module"
    ]
  },
  "jscsConfig": {
    "esnext": true,
    "fileExtensions": [
      ".js",
      ".es6"
    ],
    "excludeFiles": [
      "node_modules/**",
      "app/helpers/uuid.es6",
      "app/helpers/details.polyfill.es6"
    ],
    "additionalRules": [
      "node_modules/jscs-jsdoc/lib/rules/*.js"
    ],
    "disallowEmptyBlocks": true,
    "disallowImplicitTypeConversion": [
      "numeric",
      "boolean",
      "binary",
      "string"
    ],
    "disallowMixedSpacesAndTabs": "smart",
    "disallowMultipleLineStrings": true,
    "disallowMultipleVarDecl": true,
    "disallowNewlineBeforeBlockStatements": true,
    "disallowPaddingNewlinesInBlocks": true,
    "disallowQuotedKeysInObjects": "allButReserved",
    "disallowSpaceAfterObjectKeys": true,
    "disallowSpaceAfterPrefixUnaryOperators": true,
    "disallowSpaceBeforePostfixUnaryOperators": true,
    "disallowSpacesInCallExpression": true,
    "disallowSpacesInsideArrayBrackets": "nested",
    "disallowTrailingWhitespace": true,
    "disallowYodaConditions": true,
    "requireBlocksOnNewline": 1,
    "requireCamelCaseOrUpperCaseIdentifiers": true,
    "requireCommaBeforeLineBreak": true,
    "requireDotNotation": true,
    "requireLineFeedAtFileEnd": true,
    "requireOperatorBeforeLineBreak": true,
    "requireParenthesesAroundIIFE": true,
    "requireSpaceAfterBinaryOperators": true,
    "requireSpaceAfterKeywords": [
      "do",
      "for",
      "if",
      "else",
      "switch",
      "case",
      "try",
      "catch",
      "void",
      "while",
      "with",
      "return",
      "typeof"
    ],
    "requireSpaceAfterLineComment": true,
    "requireSpaceBeforeBinaryOperators": true,
    "requireSpaceBeforeBlockStatements": true,
    "requireSpacesInConditionalExpression": true,
    "safeContextKeyword": [
      "self"
    ],
    "validateIndentation": "\t",
    "validateLineBreaks": "LF",
    "validateParameterSeparator": ", ",
    "validateQuoteMarks": {
      "mark": "'",
      "escape": true
    },
    "jsDoc": {
      "checkParamNames": true,
      "requireParamTypes": true,
      "checkRedundantParams": true,
      "checkReturnTypes": true,
      "checkRedundantReturns": true,
      "requireReturnTypes": true,
      "checkTypes": true,
      "checkRedundantAccess": true,
      "leadingUnderscoreAccess": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hawkrives/gobbldygook.git"
  },
  "author": "Hawken MacKay Rives",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hawkrives/gobbldygook/issues"
  },
  "homepage": "https://hawkrives.github.io/gobbldygook",
  "dependencies": {
    "bluebird": "^2.3.11",
    "colors.css": "^2.0.3",
    "event-kit": "^0.8.0",
    "humanize-plus": "^1.5.0",
    "lodash": "^2.4.1",
    "react": "^0.12.1",
    "react-dnd": "^0.6.0",
    "react-layers": "^0.3.1",
    "react-router": "^0.10.2",
    "treo": "^0.3.0"
  },
  "devDependencies": {
    "6to5": "^1.12.18",
    "6to5-browserify": "^1.2.0",
    "autoprefixer": "^3.1.0",
    "browser-sync": "^1.6.3",
    "browserify": "^6.3.2",
    "cssshrink": "0.0.5",
    "del": "^0.1.3",
    "es6-shim": "^0.20.2",
    "exorcist": "^0.1.6",
    "gulp": "^3.8.10",
    "gulp-autoprefixer": "^1.0.1",
    "gulp-jscs": "^1.3.1",
    "gulp-notify": "^2.0.0",
    "gulp-sass": "^1.1.0",
    "gulp-size": "^1.1.0",
    "gulp-symlink": "^2.0.1",
    "gulp-util": "^3.0.1",
    "jscs": "^1.8.1",
    "jscs-jsdoc": "^0.2.0",
    "mocha": "^2.0.1",
    "pretty-hrtime": "^0.2.2",
    "require-dir": "^0.1.0",
    "run-sequence": "^1.0.1",
    "should": "^4.3.0",
    "uglify-js": "^2.4.15",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^2.1.1"
  }
}
