{
  "name": "gobbldygook",
  "version": "3.0.0-alpha.13",
  "description": "A course scheduling app for St. Olaf College",
  "main": "src/index.js",
  "private": true,
  "repository": "hawkrives/gobbldygook",
  "author": "Hawken MacKay Rives",
  "license": "MIT",
  "config": {
    "browserify": "--debug -e src/index.js -t [ babelify --stage 0 ]",
    "uglify": "--source-map-root ./build/ --in-source-map build/app.js.map --source-map build/app.min.js.map --compress --mangle --screw-ie8"
  },
  "scripts": {
    "cloc": "cloc ./ --by-file-by-lang --include-dir=src",
    "check": "npm outdated --depth 0",
    "clean": "rm -rf build/",
    "copy-html": "cp -f src/htdocs/{index.html,infoFile.url} build/",
    "copy-fonts": "cp -fR src/fonts build/",
    "copy-icons": "mkdir -p build/fonts/ && cp -f src/icons/font/ionicons.woff build/fonts/",
    "precopy": "mkdir -p build/",
    "copy": "npm run copy-html & npm run copy-fonts & npm run copy-icons; wait",
    "build-scripts": "browserify $npm_package_config_browserify | exorcist build/app.js.map > build/app.js",
    "build-styles": "./bin/sass.js src/styles/app.scss build/app.css",
    "prebuild": "mkdir -p build/ & npm run lint; wait",
    "build": "npm run copy & npm run build-scripts & npm run build-styles; wait",
    "watch-scripts": "watchify --verbose -o \"exorcist build/app.js.map > build/app.js\" $npm_package_config_browserify",
    "watch-styles": "nodemon --watch src/styles --ext scss --exec npm run-script build-styles",
    "watch-lint": "nodemon --watch app --watch test --ext js --exec npm run lint",
    "prewatch": "mkdir -p build/ & npm run lint; wait",
    "watch": "npm run copy & npm run watch-lint & npm run watch-scripts & npm run watch-styles; wait",
    "browser-sync": "browser-sync start --config ./bs-config.js",
    "uglify": "uglifyjs build/app.js --output build/app.min.js $npm_package_config_uglify",
    "eslint": "eslint src/",
    "lint": "npm run eslint; wait",
    "test": "jest",
    "start": "npm run serve",
    "serve": "npm run watch & npm run browser-sync; wait",
    "travis": "npm run lint && npm run build",
    "production": "npm run clean && npm run build && npm run uglify; wait"
  },
  "dependencies": {
    "babel-core": "5.1.11",
    "bluebird": "2.9.24",
    "classnames": "1.2.1",
    "combinations-generator": "1.0.1",
    "debug": "2.1.3",
    "humanize-plus": "1.5.0",
    "idb-range": "2.3.0",
    "immutable": "3.7.2",
    "keymage": "1.1.0",
    "lodash": "3.7.0",
    "marked": "0.3.3",
    "node-uuid": "1.4.3",
    "present": "0.0.6",
    "react": "0.13.2",
    "react-components": "0.2.0",
    "react-dnd": "0.9.8",
    "react-input-autosize": "0.4.3",
    "react-list-select": "0.3.0",
    "react-router": "0.13.2",
    "reflux": "0.2.7",
    "stickyfill": "1.1.1",
    "sto-areas": "2.3.12",
    "sto-helpers": "4.2.11",
    "sto-sis-time-parser": "2.0.0",
    "treo": "0.5.0",
    "whatwg-fetch": "0.7.0"
  },
  "devDependencies": {
    "autoprefixer-core": "5.1.11",
    "babel-eslint": "3.0.1",
    "babel-jest": "5.0.1",
    "babelify": "6.0.2",
    "browser-sync": "2.6.4",
    "browserify": "9.0.8",
    "dom-storage": "2.0.1",
    "eslint": "0.20.0",
    "eslint-plugin-react": "2.2.0",
    "exorcist": "0.3.0",
    "idb-polyfill": "0.0.2",
    "node-sass": "2.1.1",
    "nodemon": "1.3.7",
    "postcss": "4.1.5",
    "postcss-calc": "4.0.1",
    "postcss-colormin": "1.2.2",
    "postcss-discard-font-face": "1.0.1",
    "postcss-font-family": "1.1.0",
    "pretty-hrtime": "1.0.0",
    "uglify-js": "2.4.20",
    "watchify": "3.1.2"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testDirectoryName": "test",
    "testPathDirs": [
      "test"
    ]
  }
}
