{
  "name": "gobbldygook",
  "version": "3.0.0alpha4",
  "description": "A course scheduling app for St. Olaf College",
  "main": "build/app.js",
  "browser": "build/index.html",
  "scripts": {
    "test": "jest"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "jest": {
    "scriptPreprocessor": "preprocess-for-jest"
  },
  "jshintConfig": {
      "asi": true,
      "browser": true,
      "curly": false,
      "devel": true,
      "expr": true,
      "eqeqeq": true,
      "esnext": true,
      "globalstrict": true,
      "indent": 4,
      "loopfunc": true,
      "node": false,
      "trailing": true,
      "newcap": false,
      "quotmark": "single",
      "strict": true,
      "undef": true,
      "white": true,
      "predef": ["require", "module"]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hawkrives/gobbldygook.git"
  },
  "author": "Hawken MacKay Rives",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hawkrives/gobbldygook/issues"
  },
  "homepage": "https://hawkrives.github.io/gobbldygook",
  "devDependencies": {
    "6to5": "^1.9.9",
    "6to5-browserify": "0.0.10",
    "autoprefixer": "^3.1.0",
    "browserify": "^6.0.3",
    "colors.css": "^1.0.0",
    "http-server": "^0.7.2",
    "jest-cli": "^0.1.18",
    "node-sass": "^0.9.6",
    "watchify": "^2.0.0"
  },
  "dependencies": {
    "bluebird": "^2.3.5",
    "crypto-browserify": "^3.2.7",
    "fluxxor": "^1.4.2",
    "humanize-plus": "^1.5.0",
    "immutable": "^2.3.0",
    "lodash": "^2.4.1",
    "react": "^0.11.2",
    "treo": "^0.3.0"
  }
}
