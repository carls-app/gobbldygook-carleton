{
  "name": "gobbldygook",
  "version": "3.0.0alpha10",
  "description": "A course scheduling app for St. Olaf College",
  "main": "dist/app.js",
  "browser": "dist/index.html",
  "private": true,
  "repository": "hawkrives/gobbldygook",
  "author": "Hawken MacKay Rives",
  "license": "MIT",
  "scripts": {
    "check": "npm outdated --depth 0",
    "clean": "rm -rf dist/",
    "cloc": "cloc ./ --by-file-by-lang --exclude-dir=dist,gulp,node_modules,playground,test",
    "fix-gulp-watch": "cd node_modules/gulp/node_modules/vinyl-fs/node_modules/glob-watcher/node_modules/gaze/node_modules/globule && npm i lodash@latest",
    "fix-browser-sync": "cd node_modules/browser-sync/node_modules/glob-watcher/node_modules/gaze/node_modules/globule && npm i lodash@latest",
    "fix": "npm run fix-gulp-watch && npm run fix-browser-sync",
    "gulp": "gulp",
    "build": "gulp build",
    "lint": "gulp lint",
    "serve": "gulp watch",
    "watch": "gulp watch",
    "test": "jest",
    "travis": "npm run build && npm run lint && npm run test"
  },
  "dependencies": {
    "bluebird": "2.8.2",
    "deep-eql": "0.1.3",
    "humanize-plus": "1.5.0",
    "immutable": "3.6.2",
    "keymage": "1.1.0",
    "lodash": "2.4.1",
    "marked": "0.3.2",
    "node-uuid": "1.4.2",
    "react": "0.12.2",
    "react-dnd": "0.6.2",
    "react-router": "0.11.6",
    "reflux": "0.2.3",
    "stickyfill": "1.1.1",
    "sto-areas": "2.3.7",
    "sto-courses": "3.0.3",
    "sto-helpers": "3.0.6",
    "sto-sis-time-parser": "1.0.8",
    "treo": "^0.4.1",
    "whatwg-fetch": "0.6.1"
  },
  "devDependencies": {
    "6to5-core": "2.13.5",
    "6to5-jest": "^1.0.0",
    "6to5ify": "3.1.2",
    "autoprefixer-core": "5.0.0",
    "browser-sync": "1.9.1",
    "browserify": "8.1.1",
    "csswring": "3.0.0",
    "exorcist": "0.1.6",
    "gulp": "3.8.10",
    "gulp-cached": "1.0.2",
    "gulp-jscs": "1.4.0",
    "gulp-notify": "2.1.0",
    "gulp-postcss": "4.0.2",
    "gulp-sass": "1.3.2",
    "gulp-size": "1.2.0",
    "gulp-sourcemaps": "1.3.0",
    "gulp-util": "3.0.2",
    "jest-cli": "^0.2.1",
    "jscs-jsdoc": "0.4.0",
    "mold-source-map": "^0.3.0",
    "postcss": "4.0.1",
    "pretty-hrtime": "1.0.0",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.0.0",
    "watchify": "2.2.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/6to5-jest",
    "testDirectoryName": "test",
    "testPathDirs": [
      "test"
    ]
  }
}
