{
  "name": "gobbldygook",
  "version": "3.0.0-alpha.13",
  "description": "A course scheduling app for St. Olaf College",
  "main": "src/index.js",
  "private": true,
  "repository": "hawkrives/gobbldygook",
  "author": "Hawken MacKay Rives",
  "license": "MIT",
  "config": {
    "beautify": "-f - --end-with-newline --indent-size 2",
    "browserify": "--debug -e src/index.js -t babelify",
    "mocha": "test/areas test/models",
    "uglify": "--source-map-root ./build/ --in-source-map build/app.js.map --source-map build/app.min.js.map --compress --mangle --screw-ie8"
  },
  "scripts": {
    "benchmark": "babel-node ./benchmarks/students-benchmark.js --runs 50 --graph",
    "browser-sync": "browser-sync start --config ./bs-config.js",
    "build": "concurrent 'npm run build:scripts' 'npm run build:styles' 'npm run build:peg'",
    "build:areas": "./bin/compile-areas areas/ --out-dir dist/",
    "build:peg": "pegjs < src/lib/parse-hanson-string.pegjs | babel --compact 'true' | js-beautify $npm_package_config_beautify > src/lib/parse-hanson-string.js",
    "build:production": "npm run clean && npm run build && npm run uglify",
    "build:scripts": "browserify $npm_package_config_browserify | exorcist build/app.js.map > build/app.js",
    "build:styles": "babel-node ./bin/sass.js src/styles/app.scss build/app.css",
    "clean": "rm -rf build/",
    "copy": "concurrent 'npm run copy:html' 'npm run copy:fonts' 'npm run copy:icons'",
    "copy:fonts": "mkdirp build && cp -fR src/fonts build/",
    "copy:html": "mkdirp build && cp -f src/htdocs/index.html src/htdocs/infoFile.url src/htdocs/areaData.url build/",
    "copy:icons": "mkdirp build/fonts/ && cp -f src/icons/font/ionicons.woff build/fonts/",
    "lint": "esw bin/ src/",
    "loc": "sloc src/",
    "prebuild": "npm run copy && npm run lint",
    "prewatch": "npm run copy && npm run lint",
    "start": "concurrent 'npm run watch' 'npm run browser-sync'",
    "profile": "cat profile/README.md",
    "test": "mocha $npm_package_config_mocha",
    "test:students": "mocha ./bin/test-students",
    "travis": "npm run test:students && npm run test",
    "uglify": "uglifyjs build/app.js --output build/app.min.js $npm_package_config_uglify",
    "watch": "concurrent 'npm run watch:lint' 'npm run watch:scripts' 'npm run watch:styles'",
    "watch:lint": "npm run lint -- -w",
    "watch:scripts": "watchify --verbose -o \"exorcist build/app.js.map > build/app.js\" $npm_package_config_browserify",
    "watch:styles": "nodemon --watch src/styles --ext scss --exec 'npm run build:styles'"
  },
  "dependencies": {
    "babel-core": "5.6.20",
    "bluebird": "2.9.34",
    "classnames": "2.1.3",
    "combinations-generator": "1.0.1",
    "debug": "2.2.0",
    "humanize-list": "1.0.0",
    "humanize-plus": "1.5.0",
    "idb-range": "2.4.0",
    "immutable": "3.7.4",
    "js-yaml": "3.3.1",
    "json-stable-stringify": "1.0.0",
    "keymage": "1.1.0",
    "lodash": "3.10.0",
    "marked": "0.3.3",
    "node-uuid": "1.4.3",
    "plur": "2.0.0",
    "present": "0.0.6",
    "react": "0.13.3",
    "react-dnd": "0.9.8",
    "react-input-autosize": "0.5.0",
    "react-list-select": "0.3.0",
    "react-router": "0.13.3",
    "reflux": "0.2.11",
    "stickyfill": "1.1.1",
    "sto-areas": "2.3.12",
    "sto-helpers": "4.2.11",
    "sto-sis-time-parser": "2.0.0",
    "treo": "0.5.0",
    "whatwg-fetch": "0.9.0"
  },
  "devDependencies": {
    "autoprefixer-core": "5.2.1",
    "babel": "5.6.23",
    "babel-eslint": "3.1.26",
    "babelify": "6.1.3",
    "browser-sync": "2.8.0",
    "browserify": "11.0.0",
    "chai": "3.2.0",
    "concurrently": "0.1.1",
    "dom-storage": "2.0.1",
    "eslint": "0.24.1",
    "eslint-plugin-react": "3.0.0",
    "eslint-watch": "1.2.4",
    "exorcist": "0.4.0",
    "graceful-fs": "4.1.2",
    "js-beautify": "1.5.10",
    "junk": "1.0.2",
    "meow": "3.3.0",
    "mkdirp": "0.5.1",
    "mocha": "2.2.5",
    "node-sass": "2.1.1",
    "nodemon": "1.3.8",
    "pegjs": "0.8.0",
    "postcss": "4.1.16",
    "postcss-calc": "4.1.0",
    "postcss-colormin": "1.2.3",
    "postcss-discard-font-face": "1.0.1",
    "postcss-font-family": "1.1.1",
    "pretty-hrtime": "1.0.0",
    "pretty-ms": "1.4.0",
    "sloc": "0.1.9",
    "sparkly": "2.0.1",
    "uglify-js": "2.4.24",
    "watchify": "3.3.0"
  }
}
