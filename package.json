{
  "name": "gobbldygook",
  "version": "3.0.0alpha4",
  "description": "A course scheduling app for St. Olaf College",
  "main": "build/app.js",
  "browser": "build/index.html",
  "scripts": {
    "test": "jest"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "node_modules",
      "data"
    ],
    "unmockedModulePathPatterns": [
      "lodash"
    ],
    "scriptPreprocessor": "preprocess-for-jest"
  },
  "jshintConfig": {
    "asi": true,
    "browser": true,
    "curly": false,
    "devel": true,
    "expr": true,
    "eqeqeq": true,
    "esnext": true,
    "globalstrict": true,
    "indent": 4,
    "loopfunc": true,
    "node": false,
    "trailing": true,
    "newcap": false,
    "quotmark": "single",
    "strict": true,
    "undef": true,
    "white": true,
    "predef": [
      "require",
      "module"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hawkrives/gobbldygook.git"
  },
  "author": "Hawken MacKay Rives",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hawkrives/gobbldygook/issues"
  },
  "homepage": "https://hawkrives.github.io/gobbldygook",
  "devDependencies": {
    "6to5": "^1.9.9",
    "6to5-browserify": "0.0.10",
    "autoprefixer": "^3.1.0",
    "browser-sync": "^1.5.8",
    "browserify": "^6.0.3",
    "colors.css": "^1.0.0",
    "del": "^0.1.3",
    "gulp": "^3.8.8",
    "gulp-autoprefixer": "^1.0.1",
    "gulp-changed": "^1.0.0",
    "gulp-if": "^1.2.5",
    "gulp-jshint": "^1.8.5",
    "gulp-notify": "^2.0.0",
    "gulp-sass": "^1.2.0",
    "gulp-size": "^1.1.0",
    "gulp-sourcemaps": "^1.2.4",
    "jest-cli": "^0.1.18",
    "jshint-stylish": "^1.0.0",
    "run-sequence": "^1.0.1",
    "vinyl-source-stream": "^1.0.0",
    "reflux": "^0.1.13",
    "watchify": "^2.0.0"
  },
  "dependencies": {
    "bluebird": "^2.3.5",
    "crypto-browserify": "^3.2.7",
    "fluxxor": "^1.4.2",
    "humanize-plus": "^1.5.0",
    "immutable": "^2.3.0",
    "lodash": "^2.4.1",
    "react": "^0.11.2",
    "treo": "^0.3.0"
  }
}
