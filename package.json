{
  "name": "gobbldygook",
  "version": "3.0.0alpha8",
  "description": "A course scheduling app for St. Olaf College",
  "main": "build/app.js",
  "browser": "build/index.html",
  "private": true,
  "scripts": {
    "test": "mocha",
    "start": "make build"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "jshintConfig": {
    "asi": true,
    "browser": true,
    "curly": false,
    "devel": true,
    "expr": true,
    "eqeqeq": true,
    "esnext": true,
    "globalstrict": true,
    "indent": "\t",
    "loopfunc": true,
    "node": false,
    "trailing": true,
    "newcap": false,
    "quotmark": "single",
    "strict": true,
    "undef": true,
    "white": true,
    "predef": [
      "require",
      "module"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hawkrives/gobbldygook.git"
  },
  "author": "Hawken MacKay Rives",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hawkrives/gobbldygook/issues"
  },
  "homepage": "https://hawkrives.github.io/gobbldygook",
  "dependencies": {
    "bluebird": "^2.3.11",
    "colors.css": "^2.0.3",
    "event-kit": "^0.8.0",
    "fetch": "git://github.com/github/fetch",
    "humanize-plus": "^1.5.0",
    "immutable": "^3.4.1",
    "lodash": "^2.4.1",
    "react": "^0.12.1",
    "react-dnd": "^0.6.1",
    "react-router": "^0.11.4",
    "sto-courses": "^2.0.2014-11-30.8",
    "treo": "^0.3.0"
  },
  "devDependencies": {
    "browser-sync": "^1.7.2",
    "browserify": "^6.3.3",
    "6to5ify": "^2.0.0",
    "autoprefixer-core": "4.0.2",
    "csswring": "^2.0.0",
    "exorcist": "^0.1.6",
    "gulp": "^3.8.10",
    "gulp-jscs": "^1.3.1",
    "gulp-notify": "^2.0.1",
    "gulp-postcss": "^3.0.0",
    "gulp-sass": "^1.1.0",
    "gulp-size": "^1.1.0",
    "gulp-sourcemaps": "^1.2.8",
    "gulp-symlink": "^2.1.0",
    "gulp-util": "^3.0.1",
    "jscs": "^1.8.1",
    "jscs-jsdoc": "^0.3.2",
    "mocha": "^2.0.1",
    "pretty-hrtime": "^0.2.2",
    "require-dir": "^0.1.0",
    "run-sequence": "^1.0.2",
    "should": "^4.3.0",
    "uglify-js": "^2.4.15",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^2.1.1"
  }
}
