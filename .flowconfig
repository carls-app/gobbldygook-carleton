[version]
^0.31.0


[ignore]
.*\.json

.*/node_modules/fbjs
.*/node_modules/.*/node_modules/fbjs


[libs]
./node_modules/fbjs/flow/lib
./node_modules/iflow-bluebird/index.js.flow
./node_modules/iflow-lodash/index.js.flow


[options]
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable

module.name_mapper='^\(.*\)\.css$' -> '<PROJECT_ROOT>/config/flow/css'
module.name_mapper='^\(.*\)\.scss$' -> '<PROJECT_ROOT>/config/flow/css'
module.name_mapper='^\(.*\)\.\(jpg\|png\|gif\|eot\|otf\|webp\|svg\|ttf\|woff\|woff2\|mp4\|webm\)$' -> '<PROJECT_ROOT>/config/flow/file'

suppress_type=$FlowIssue
suppress_type=$FlowFixMe
